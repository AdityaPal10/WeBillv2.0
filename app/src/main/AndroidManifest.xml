<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.teamblue.WeBillv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="We Bill"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.teamblue.WeBillv2.view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.teamblue.WeBillv2.view.SIgnUpView"
            android:parentActivityName="com.teamblue.WeBillv2.view.MainActivity" />
        <activity
            android:name="com.teamblue.WeBillv2.view.MyAccountView"
            android:parentActivityName="com.teamblue.WeBillv2.view.MainActivity" />
        <activity
            android:name="com.teamblue.WeBillv2.view.AddBillView"
            android:parentActivityName="com.teamblue.WeBillv2.view.MainActivity" />
        <activity
            android:name="com.teamblue.WeBillv2.view.FriendsView"
            android:parentActivityName="com.teamblue.WeBillv2.view.MainActivity" />
    </application>

</manifest>